{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "900px",
			"wideSize": "1300px"
		},
		"color": {
			"customGradient": true,
			"palette": [
				{
					"color": "#1A86B2",
					"name": "Light Blue",
					"slug": "light-blue"
				},
				{
					"color": "#16467B",
					"name": "Dark Blue",
					"slug": "dark-blue"
				},
				{
					"color": "#F5F5F5",
					"name": "Snow White",
					"slug": "snow-white"
				},
				{
					"color": "#F9FAFB",
					"name": "Off White",
					"slug": "off-white"
				},
				{
					"color": "#202020",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "clamp(5px, 1vw, 10px)",
					"slug": "tiny"
				},
				{
					"name": "X-Small",
					"size": "clamp(16px, 2vw, 25px)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(30px, 4vw, 40px)",
					"slug": "small"
				},
				{
					"name": "Regular",
					"size": "clamp(40px, 6vw, 60px)",
					"slug": "regular"
				},
				{
					"name": "Large",
					"size": "clamp(50px, 8vw, 80px)",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "clamp(60px, 10vw, 100px)",
					"slug": "x-large"
				},
				{
					"name": "XX-Large",
					"size": "clamp(70px, 10vw, 140px)",
					"slug": "xx-large"
				}
			],
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"writingMode": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": false,
					"name": "X Small",
					"size": "0.875rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Medium",
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2.11rem",
						"min": "1.56rem"
					},
					"name": "Large",
					"size": "1.56rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.81rem",
						"min": "1.95rem"
					},
					"name": "Extra Large",
					"size": "1.95rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3.75rem",
						"min": "2.44rem"
					},
					"name": "Bombo Claat",
					"size": "2.44rem",
					"slug": "bombo-claat"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
